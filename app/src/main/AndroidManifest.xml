<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="emplay.entertainment.emplay">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/e_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/e_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.EMPlay">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MusicOption"></activity>
        <activity android:name=".MovieOption"></activity>
        <activity android:name=".AboutPage"></activity>
        <activity android:name=".LoginPage"></activity>
        <activity android:name=".SignUpPage"></activity>

    </application>


</manifest>